float4x4 IvModelViewProjectionMatrix;
struct VS_OUTPUT
{
	float4 pos : POSITION;
	float2 uv : TEXCOORD0;
};


VS_OUTPUT vs_main( float2 uv : TEXCOORD0, float4 pos : POSITION )
{
    VS_OUTPUT Out = (VS_OUTPUT) 0;

    Out.pos = mul(pos, IvModelViewProjectionMatrix);

    Out.uv = uv;

    return Out;
}
